{
  "name": "moviefy-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "rm -rf .next out && APP_ENV=development next dev",
    "dev-prod": "rm -rf .next out && APP_ENV=production next dev",
    "build": "rm -rf out .next && APP_ENV=development next build",
    "build-prod": "rm -rf out .next && APP_ENV=production next build",
    "start": "next start",
    "format": "npx prettier --write \"src/**/*.{ts,tsx}\" \"pages/**/*.{ts,tsx}\"",
    "export": "npm run build && next export",
    "export-prod": "npm run build-prod && next export",
    "lint": "next lint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@reactivers/next-ga": "^1.0.0",
    "next": "12.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "husky": "^8.0.3",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "ts-node": "^10.9.1",
    "typescript": "4.7.4"
  }
}
