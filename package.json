{
  "name": "moviefy-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "rm -rf .next out && APP_ENV=development next dev",
    "dev-prod": "rm -rf .next out && APP_ENV=production next dev",
    "build": "rm -rf out .next && APP_ENV=development next build",
    "build-prod": "rm -rf out .next && APP_ENV=production next build",
    "start": "next start",
    "export": "npm run build && next export",
    "export-prod": "npm run build-prod && next export",
    "generate": "APP_ENV=development graphql-codegen --config codegen.js",
    "generate-prod": "APP_ENV=production graphql-codegen --config codegen.js",
    "lint": "next lint"
  },
  "dependencies": {
    "@reactivers/next-ga": "^1.0.0",
    "next": "12.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "ts-node": "^10.9.1",
    "typescript": "4.7.4"
  }
}